// Top header start
header#top-header-2.top-header.th-bg
  .container
    .row
      .col-lg-9.col-md-9.col-sm-7
        .list-inline
          if user
            a
              i.fa.fa-phone
              | #{user.phone ? user.phone : "xxx"}
          if user
            a
              i.fa.fa-envelope
              | #{user.email}
      .col-lg-3.col-md-3.col-sm-5
        ul.top-social-media.pull-right
          if !user
            li
              a.sign-in(href='/login')
                i.fa.fa-sign-in
                |  Login
            li  
              a.sign-in(href='/register')
                i.fa.fa-user
                |  Signup
          if user
            li
              a.sign-in(href=`/user`)
                i.fa.fa-user
                |  profile
            li       
              a.sign-in.logout--btn
                i.fa.fa-user
                |  Logout
